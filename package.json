{
  "name": "express-jwt",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.11.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "eslint": "eslint src",
    "lint": "eslint --fix src",
