    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-group-routes": "^1.1.0",
    "express-jwt": "^8.4.1",
    "express-validator": "^7.0.1",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.0",
    "sequelize-auto": "^0.8.8",
    "sequelize6-auto-migrations": "^1.0.1",
    "serve-favicon": "^2.5.0",
