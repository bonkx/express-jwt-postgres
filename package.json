    "compression-webpack-plugin": "^11.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.9.1",
    "sequelize-cli": "^6.6.2",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
